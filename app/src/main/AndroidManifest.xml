<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vison.finance.financemanager">

    <application
        android:name=".framework.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity
            android:name=".biz.project.view.NewProjectActivity"
            android:label="@string/title_activity_new_project"
            android:theme="@style/AppTheme" />
        <activity android:name=".biz.category.view.CategoryActivity" />
        <activity android:name=".biz.category.view.NewCategoryActivity" />
        <activity android:name=".biz.money.view.NewMoneyActivity" />
    </application>

</manifest>